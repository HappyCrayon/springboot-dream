<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.admin.mapper.ProductMapper">

    <insert id="insertProd">
        insert into product(prod_id, prod_name) VALUES(#{prod.prodId}, #{prod.prodName})
    </insert>

    <insert id="insertProdFor">
        <foreach collection="prodList" item="prod" separator=";">
            insert into product(prod_id, prod_name) VALUES(#{prod.prodId}, #{prod.prodName})
        </foreach>
    </insert>

</mapper>
